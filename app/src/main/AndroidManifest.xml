            android:name=".DashboardActivity.SignUpActivity"
            android:exported="false" />
        <activity
            android:name=".DashboardActivity.FloatingActionButton"
            android:exported="false" />
        <activity
            android:name=".DashboardActivity.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
